<h1>Listing users</h1>

<table>
  <tr>
    <th>Fbuser</th>
    <th>Current question</th>
    <th>Isadmin</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.fbUser %></td>
    <td><%= user.current_question_id %></td>
    <td><%= user.isAdmin %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

